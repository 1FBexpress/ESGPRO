{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/daily-analytics-report",
      "schedule": "0 9 * * *"
    }
  ],
  "github": {
    "silent": false
  }
}
